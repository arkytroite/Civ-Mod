	<GameData>
	
		<!-- Edits the Polder build action so that it may be built while embarked, and changes the required technology from Guilds to Machinery -->
		
		<Builds>
			<Update>
				<Where Type="BUILD_POLDER" />
				<Set Kill= "true" Water="true" CanBeEmbarked="true" PrereqTech="TECH_MACHINERY" EntityEvent="ENTITY_EVENT_BUILD" />
			</Update>
		</Builds>

	</GameData>